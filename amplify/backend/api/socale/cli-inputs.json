{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "socale",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "expirationTime": 7
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "OPTIMISTIC_CONCURRENCY"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "OPTIMISTIC_CONCURRENCY"
          },
          "entityName": "Match"
        },
        {
          "resolutionStrategy": {
            "type": "OPTIMISTIC_CONCURRENCY"
          },
          "entityName": "User"
        },
        {
          "resolutionStrategy": {
            "type": "OPTIMISTIC_CONCURRENCY"
          },
          "entityName": "UserRoom"
        },
        {
          "resolutionStrategy": {
            "type": "OPTIMISTIC_CONCURRENCY"
          },
          "entityName": "Room"
        },
        {
          "resolutionStrategy": {
            "type": "OPTIMISTIC_CONCURRENCY"
          },
          "entityName": "Message"
        }
      ]
    }
  }
}